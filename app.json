{
  "expo": {
    "name": "VOP-APP",
    "slug": "VOP-APP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/netIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#333",
      "image": "./assets/images/netIcon.png",
      "width": 100,
      "height": 100
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSCameraUsageDescription": "This app uses the camera to allow video playback.",
        "NSMicrophoneUsageDescription": "This app uses the microphone to allow video playback."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/netIcon.png",
        "backgroundColor": "#333"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/netIcon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#333",
          "dark": {
            "image": "./assets/splash-icon-dark.png",
            "backgroundColor": "#333"
          }
        }
      ],
      "expo-av"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
